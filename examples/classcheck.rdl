interface F (~M : Type) {
  zero : ~M;;
  one  : ~M -> ~M;;
  two : ~M -> ~M -> ~M;;
};;

impl Int for F {
  zero := 0;;
  one x := 1;;
  two a b := 2;;
};;

impl Bool for F {
  zero := False;;
  one a := True;;
  two b a := False;;
};;

impl ~A is F | Option ~A for F {
  zero := None;;
  one (Some a) := Some a;;
  two (Some a) (Some b) := Some b;;
};;


f := one;;
ff := two;;

